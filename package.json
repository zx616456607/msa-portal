{
  "name": "msa-portal",
  "version": "0.1.0",
  "description": "portal for micro service",
  "main": "server/index.js",
  "scripts": {
    "analyze": "cross-env ANALYZE_ENV=true npm run build",
    "analyze:pro": "cross-env ANALYZE_ENV=true npm run build:pro",
    "build": "webpack --config webpack_config/client.js --progress --colors",
    "build:dll": "webpack --config webpack_config/dll.js --progress --colors",
    "build:pro": "cross-env NODE_ENV=production npm run build",
    "build-server": "webpack --config webpack_config/server.js --progress --colors",
    "build-server:pro": "cross-env NODE_ENV=production npm run build-server",
    "clean": "rm -rf dist static/public index.html",
    "client": "cross-env NODE_ENV=development node server/index.js",
    "dev": "cross-env NODE_ENV=development npm start",
    "i": "npm install --registry=https://registry.npm.taobao.org",
    "lint": "npm run lint:es && npm run lint:style",
    "lint:es": "eslint .",
    "lint:style": "stylelint \"client/{containers,components,assets}/**/*.less\" --syntax less",
    "lint-fix": "npm run lint-fix:es && npm run lint-fix:style",
    "lint-fix:es": "eslint . --fix",
    "lint-fix:style": "stylelint \"client/{containers,components,assets}/**/*.less\" --syntax less --fix",
    "pro": "cross-env NODE_ENV=production npm start",
    "pre-commit-msg": "echo 'Pre-commit checks...' && exit 0",
    "start": "node server/index.js | bunyan -o short",
    "test": "cross-env BABEL_ENV=commonjs jest"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.tenxcloud.com:enterprise-2.0/msa-portal.git"
  },
  "keywords": [
    "node.js",
    "koa",
    "react",
    "redux"
  ],
  "pre-commit": [
    "pre-commit-msg",
    "lint"
  ],
  "author": "zhangpc@tenxcloud.com",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "bluebird": "^3.4.7",
    "koa": "^2.4.1",
    "koa-bunyan-logger": "^2.0.0",
    "koa-ejs": "^4.0.0",
    "koa-router": "^7.3.0",
    "koa-static": "^4.0.2",
    "lodash": "^4.17.4",
    "moment": "^2.19.2",
    "query-string": "^5.0.1",
    "urllib": "^2.25.1",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@antv/g6": "1.1.1",
    "antd": "^3.0.2",
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.23.1",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.2",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "classnames": "^2.2.5",
    "codemirror": "^5.31.0",
    "compression-webpack-plugin": "^1.0.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.26.2",
    "eslint": "^4.12.0",
    "eslint-config-egg": "^5.1.1",
    "eslint-plugin-react": "^7.5.1",
    "expect": "^21.2.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "fetch-mock": "^5.13.1",
    "file-loader": "^0.10.1",
    "g2": "^2.3.13",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.29.0",
    "jest": "^21.2.1",
    "koa-webpack": "^1.0.0",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "normalizr": "^3.2.2",
    "postcss-loader": "^2.0.8",
    "pre-commit": "^1.2.2",
    "prop-types": "^15.5.10",
    "rc-queue-anim": "^1.4.0",
    "react": "^16.2.0",
    "react-codemirror2": "^3.0.7",
    "react-dock": "^0.2.4",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.0",
    "redux-mock-store": "^1.3.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "style-loader": "^0.18.1",
    "stylelint": "^8.3.1",
    "stylelint-config-standard": "^18.0.0",
    "svg-sprite-loader": "^3.4.1",
    "svgo": "^1.0.3",
    "svgo-loader": "^2.1.0",
    "url-loader": "^0.5.8",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-md5-hash": "0.0.5"
  },
  "engines": {
    "node": ">= 7.6.0"
  },
  "jest": {
    "setupFiles": [
      "./client/test/.setup.js"
    ]
  }
}
